<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.Scene?>

<AnchorPane xmlns:fx="http://javafx.com/fxml" fx:controller="com.example.LoginController">
    <children>
        <Label text="Login Page" layoutX="100" layoutY="20"/>
        <TextField fx:id="usernameField" promptText="Username" layoutX="100" layoutY="60"/>
        <PasswordField fx:id="passwordField" promptText="Password" layoutX="100" layoutY="100"/>
        <Button text="Login" layoutX="100" layoutY="140" onAction="#handleLogin"/>
        <Label fx:id="messageLabel" layoutX="100" layoutY="180"/>
    </children>
</AnchorPane>
